    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "runtimeExecutable": "C:/Users/Shrine/AppData/Local/Chromium/Application/chrome.exe",
            "file": "${workspaceFolder}/Randomizer/ArknightsRandomizer.html"
        },
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "pwa-chrome",
            "webRoot": "${workspaceFolder}"
        }
    ]
}